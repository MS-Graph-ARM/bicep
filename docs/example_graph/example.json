{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "languageVersion": "1.9-experimental",
  "contentVersion": "1.0.0.0",
  "metadata": {
    "EXPERIMENTAL_WARNING": "Symbolic name support in ARM is experimental, and should be enabled for testing purposes only. Do not enable this setting for any production usage, or you may be unexpectedly broken at any time!",
    "_generator": {
      "name": "bicep",
      "version": "0.7.23.25540",
      "templateHash": "17320741973996287505"
    }
  },
  "imports": {
    "graphNamespace": {
      "provider": "Graph",
      "version": "1.0",
      "config": {
        "graphToken": "graphToken"
      }
    }
  },
  "resources": {
    "myGroup2::anotherUser": {
      "import": "graphNamespace",
      "type": "Microsoft.Graph/groups/members@2022-06-15-preview",
      "properties": {
        "name": "anotherUserName",
        "@odata.id": "[format('https://graph.microsoft.com/v1.0/directoryObjects/{0}', reference('user1').id)]"
      },
      "dependsOn": [
        "myGroup2",
        "user1"
      ]
    },
    "testSP::roleAssignment": {
      "import": "graphNamespace",
      "type": "Microsoft.Graph/servicePrincipals/appRoleAssignments@2022-06-15-preview",
      "properties": {
        "name": "roleAssignmentName",
        "appRoleId": "appRoleId",
        "principalId": "[reference('testSP').id]",
        "resourceId": "resourceId"
      },
      "dependsOn": [
        "testSP"
      ]
    },
    "user1": {
      "import": "graphNamespace",
      "type": "Microsoft.Graph/users@2022-06-15-preview",
      "properties": {
        "name": "user1",
        "accountEnabled": true,
        "displayName": "user1DisplayName",
        "mailNickname": "user1MailNickname",
        "onPremisesImmutableId": "someId",
        "userPrincipalName": "user1PrincipalName",
        "passwordProfile": {
          "forceChangePasswordNextSignIn": false,
          "forceChangePasswordNextSignInWithMfa": false,
          "password": "user1Password"
        }
      }
    },
    "existingUser": {
      "existing": true,
      "type": "Microsoft.ManagedIdentity/userAssignedIdentities",
      "apiVersion": "2021-09-30-preview",
      "name": "my-managed-identity"
    },
    "myGroup2": {
      "import": "graphNamespace",
      "type": "Microsoft.Graph/groups@2022-06-15-preview",
      "properties": {
        "name": "myGroup2",
        "displayName": "myGroup2",
        "mailEnabled": false,
        "mailNickName": "myGroup2",
        "securityEnabled": true
      }
    },
    "groupWithNewMember": {
      "import": "graphNamespace",
      "type": "Microsoft.Graph/groups/members@2022-06-15-preview",
      "properties": {
        "name": "groupWithNewMember",
        "@odata.id": "[format('https://graph.microsoft.com/v1.0/directoryObjects/{0}', resourceInfo('existingUser').id)]"
      }
    },
    "testApp": {
      "import": "graphNamespace",
      "type": "Microsoft.Graph/applications@2022-06-15-preview",
      "properties": {
        "name": "testAppName",
        "displayName": "testAppDisplayName"
      }
    },
    "testSP": {
      "import": "graphNamespace",
      "type": "Microsoft.Graph/servicePrincipals@2022-06-15-preview",
      "properties": {
        "name": "testSPName",
        "appId": "[reference('testApp').appId]"
      },
      "dependsOn": [
        "testApp"
      ]
    },
    "exampleStorage": {
      "type": "Microsoft.Storage/storageAccounts",
      "apiVersion": "2021-09-01",
      "name": "storageName",
      "location": "location",
      "sku": {
        "name": "storageAccountSkuName"
      },
      "properties": {
        "accessTier": "Cool"
      },
      "kind": "StorageV2"
    }
  }
}